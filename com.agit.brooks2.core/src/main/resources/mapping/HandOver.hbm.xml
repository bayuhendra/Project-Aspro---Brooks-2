<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class mutable="true" name="com.agit.brooks2.domain.handover.HandOver" table="AGIT_HAND_OVER">
        <!--<cache usage="read-only"/>-->
        <id column="id" name="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
        </id>
        <property column="idHandOver" name="idHandOver" not-null="false"/>
        <property column="startTime" name="startTime" type="time" not-null="false"/>
        <property column="endTime" name="endTime" type="time" not-null="false"/>
        <property column="startDate" name="startDate" type="date" not-null="false"/>
        <property column="endDate" name="endDate" type="date" not-null="false"/>        
        
        <property column="createdBy" name="createdBy" not-null="false"/>
        <property column="createdDate" name="createdDate" not-null="false"/>
        <property column="modifiedBy" name="modifiedBy" not-null="false"/>
        <property column="modifiedDate" name="modifiedDate" not-null="false"/>
        <property column="status" name="status" not-null="true" update="true">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.agit.brooks2.shared.status.Status</param>
                <param name="type">12</param>
            </type>
        </property>
        
    </class>
</hibernate-mapping>
